    "@testing-library/user-event": "*",
    "babel-jest": "^29.3.1",
    "bestzip": "*",
    "jest": "^29.3.1",
    "msw": "*",
    "puppeteer": "*",
    "puppeteer-core": "*"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
